{
  "name": "codingfuture-cfbackup",
  "version": "1.1.0",
  "author": "Andrey Galkin",
  "summary": "System backup & restore management",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cfbackup",
  "project_page": "https://github.com/codingfuture/puppet-cfbackup",
  "issues_url": "https://github.com/codingfuture/puppet-cfbackup/issues",
  "tags": [
    "backup",
    "sftp",
    "s3",
    "gpg"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "18.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.7.0 <7.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "codingfuture-cfnetwork",
      "version_requirement": ">= 1.1.0 <2.0.0"
    },
    {
      "name": "codingfuture-cfsystem",
      "version_requirement": ">= 1.1.0 <2.0.0"
    }
  ],
  "data_provider": "hiera"
}
